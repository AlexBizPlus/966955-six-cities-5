{
  "name": "six-cities",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Шесть городов» от HTML Academy",
  "scripts": {
    "eslint": "eslint --ext .jsx --ext .js src/",
    "test": "npm run eslint && npm run test.jest",
    "test.jest": "jest",
    "build": "webpack --mode production",
    "start": "webpack-dev-server"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "axios-mock-adapter": "1.17.0",
    "babel-loader": "8.1.0",
    "classnames": "2.2.6",
    "css-loader": "5.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "5.13.0",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-plugin-react": "7.18.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "react-redux": "7.2.2",
    "react-test-renderer": "16.12.0",
    "redux": "4.0.5",
    "redux-mock-store": "1.5.4",
    "style-loader": "2.0.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "axios": ">=0.21.1",
    "history": "4.10.1",
    "leaflet": "1.7.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "redux-thunk": "2.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^@app(.*)": "<rootDir>/src/components/app/app.jsx",
      "^@main(.*)": "<rootDir>/src/components/main/main.jsx",
      "^@offer(.*)": "<rootDir>/src/components/offer/offer.jsx",
      "^@login(.*)": "<rootDir>/src/components/login/login.jsx",
      "^@favorites(.*)": "<rootDir>/src/components/favorites/favorites.jsx",
      "^@card(.*)": "<rootDir>/src/components/card/card.jsx",
      "^@cards(.*)": "<rootDir>/src/components/cards/cards.jsx",
      "^@form(.*)": "<rootDir>/src/components/form/form.jsx",
      "^@cityMap(.*)": "<rootDir>/src/components/city-map/city-map.jsx",
      "^@cities(.*)": "<rootDir>/src/components/cities/cities.jsx",
      "^@emptyFavorites(.*)": "<rootDir>/src/components/empty-favorites/empty-favorites.jsx",
      "^@emptyMain(.*)": "<rootDir>/src/components/empty-main/empty-main.jsx",
      "^@const(.*)": "<rootDir>/src/const.js",
      "^@prop(.*)": "<rootDir>/src/prop.js",
      "^@utils(.*)": "<rootDir>/src/utils.js",
      "^@actions(.*)": "<rootDir>/src/store/actions/$1"
    }
  }
}
